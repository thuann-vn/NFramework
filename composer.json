{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "algolia/algoliasearch-client-php": "^1.25",
        "barryvdh/laravel-ide-helper": "^2.4",
        "binarytorch/larecipe": "^1.2",
        "cartalyst/stripe-laravel": "^8.0",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "~4.0",
        "genealabs/laravel-model-caching": "^0.2.64",
        "gloudemans/shoppingcart": "^2.5",
        "guzzlehttp/guzzle": "^6.3",
        "larapack/voyager-hooks": "^1.0",
        "laravel/framework": "5.6.*",
        "laravel/horizon": "^1.4",
        "laravel/scout": "^4.0",
        "laravel/tinker": "~1.0",
        "mariuzzo/laravel-js-localization": "^1.4",
        "nicolaslopezj/searchable": "^1.9",
        "predis/predis": "^1.1",
        "pvtl/voyager-page-blocks": "^0.8.2",
        "renatomarinho/laravel-page-speed": "^1.8",
        "tcg/voyager": "1.1.*",
        "test-hook": "^1.0",
        "voyager-themes": "^0.14.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.1",
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": {
        "hooks": {
            "type": "composer",
            "url": "https://larapack.io"
        }
    }
}
